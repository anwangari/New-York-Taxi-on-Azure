{
	"name": "Parquet1_source",
	"properties": {
		"linkedServiceName": {
			"referenceName": "HttpServer1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"year": {
				"type": "string",
				"defaultValue": [
					"2019",
					"2020"
				]
			},
			"service": {
				"type": "string",
				"defaultValue": [
					"yellow",
					"green"
				]
			},
			"month": {
				"type": "string",
				"defaultValue": [
					"01",
					"02",
					"03",
					"04",
					"05",
					"06",
					"07",
					"08",
					"09",
					"10",
					"11",
					"12"
				]
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": "@{concat(pipeline().parameters.service, '_tripdata_', pipeline().parameters.year, '-', pipeline().parameters.month, '.parquet')}"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}